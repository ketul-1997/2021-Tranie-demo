<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="seq_house_order" model="ir.sequence">
            <field name="name">House Id</field>
            <field name="code">house.data</field>
            <field name="prefix">H</field>
            <field name="padding">5</field>
        </record>
        <!-- Data records go here -->
        <record id="house_form_view" model="ir.ui.view">
            <field name="name">House Form View</field>
            <field name="model">house.data</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <group>
                                <field name="name"/>
                                <field name="sequence"/>
                                <field name="society"/>
                                <field name="area"/>
                                <field name="landmark"/>
                                <field name="city"/>
                            </group>
                            <group>
                                <field name="start_price"/>
                                <field name="end_price"/>
                                <field name="availability"/>
                            </group>
                            <group>
                                <field name="landlord_id"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="house_tree_view" model="ir.ui.view">
            <field name="name">House Tree View</field>
            <field name="model">house.data</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="sequence"/>
                    <field name="society"/>
                    <field name="area"/>
                    <field name="landmark"/>
                    <field name="city"/>
                    <field name="start_price"/>
                    <field name="end_price"/>
                    <field name="availability"/>
                    <field name="landlord_id"/>
                </tree>
            </field>
        </record>

        <record id="house_search_view" model="ir.ui.view">
            <field name="name">House Search View</field>
            <field name="model">house.data</field>
            <field name="arch" type="xml">
                <search string="House Information">
                    <field name="name"/>
                    <field name="sequence"/>
                    <field name="society"/>
                    <field name="area"/>
                    <field name="landmark"/>
                    <field name="city"/>
                    <field name="start_price"/>
                    <field name="end_price"/>
                    <field name="availability"/>
                    <field name="landlord_id"/>
                </search>
            </field>
        </record>

        <record id="action_house_data" model="ir.actions.act_window">
            <field name="name">House</field>
            <field name="res_model">house.data</field>
            <field name="view_mode">tree,form</field>
        </record>

    </data>
</odoo>